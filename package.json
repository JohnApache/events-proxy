{
  "name": "eventsproxy",
  "version": "1.3.0",
  "description": "自定义事件代理",
  "main": "index.js",
  "scripts": {
    "test": "nyc --reporter=text mocha -t 1000 'test/**/*.test.@(js|jsx)'",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "build": "npm run lint && rollup -c && npm run test",
